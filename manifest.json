{
  "name": "Better Meetings",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Better meetings",
  "content_scripts": [ {
    "matches": ["http://calendar.google.com/*",
                "https://calendar.google.com/*"],
    "js": ["jquery-3.3.1.min.js",
           "bettermeetings.js"],
    "run_at": "document_end"
  }],
  "permissions": ["activeTab",
                  "https://www.google.com/" ]
}
